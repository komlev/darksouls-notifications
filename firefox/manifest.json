{
  "manifest_version": 2,
  "name": "Gmail Email Sent",
  "description": "After you send an email through gmail, you'll see Dark Souls themed notification.",
  "version": "1.1",
  "icons": {
    "48": "icon-48.png",
    "96": "icon-96.png"
  },

  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["script.js"],
      "css": ["styles.css"]
    }
  ],
  "web_accessible_resources": ["sound.mp3"]
}
